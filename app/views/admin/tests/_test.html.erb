<tr>
  <td>
    <%= test.title %>
  </td>
  <td>
    <%= test_level(test) %>
  </td>
  <td>
    <%= test.questions.count %>
  </td>
  <td>
    <%= link_to t('helpers.submit.test.show'), admin_test_path(test), class: 'link-primary' %>
  </td>
  <td>
    <%= link_to t('helpers.submit.test.edit'), edit_admin_test_path(test), class: 'link-primary' %>
  </td>
  <td>
    <%= link_to t('helpers.submit.test.delete'),
                admin_test_path(test),
                method: :delete,
                class: 'link-danger',
                data: { confirm: t('helpers.delete') } %>
  </td>
</tr>
