<tr>
  <td>
    <%= test.title %>
  </td>
  <td>
    <%= test_level(test) %>
  </td>
  <td>
    <%= test.questions.count %>
  </td>
  <td>
    <%= link_to 'show', test_path(test) %>
  </td>
  <td>
    <%= link_to 'edit', edit_test_path(test) %>
  </td>
  <td>
    <%= button_to 'Start', start_test_path(test) %>
  </td>
  <td>
    <%= link_to 'delete',
                test_path(test),
                method: :delete,
                data: { confirm: 'Are you sure?' } %>
  </td>
</tr>
